{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "powblocs",
  "version": "0.1.0",
  "identifier": "com.powblocs.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "PowBlocs",
        "maximized": true,
        "shortcuts": [
          "Alt+Enter",
          "Alt+Down",
          "Alt+F4",
          "CommandOrControl+=",
          "CommandOrControl+-",
          "CommandOrControl+0"
        ]
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
